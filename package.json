{
  "name": "commitplusplus",
  "displayName": "commit++",
  "description": "A VS Code extension to create commits with emoji",
  "author": {
    "name": "V.S.Sarath Chandra",
    "email": "vssarathc04@gmail.com"
  },
  "icon": "images/logo.png",
  "license": "MIT",
  "publisher": "VangipuramSrinivasaSarathChandra",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sarath191181208/commit--"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.52.0"
  },
  "keywords": [
    "commit++",
    "commit plus plus",
    "commitplusplus",
    "git",
    "emoji",
    "commit",
    "messages"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "type": "object",
    "title": "Commit++ configuration",
    "commands": [
      {
        "command": "extension.commit++",
        "category": "commit++",
        "title": "Create a new commit with emoji",
        "icon": {
          "dark": "images/logo.png",
          "light": "images/logo.png"
        }
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "extension.commit++",
          "group": "navigation@0"
        }
      ]
    },
    "properties": {
      "commit++.additionalCommitPrefixes": {
        "type": "array",
        "description": "Additional prefixes to be added to the list",
        "default": [],
        "items": {
          "type": "object",
          "properties": {
            "prefix": {
              "type": "string",
              "description": "Emoji or text to be added to the list"
            },
            "description": {
              "type": "string",
              "description": "Description of the commit prefix"
            }
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "ts-loader": "^9.4.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "@vscode/test-electron": "^2.3.0"
  }
}
